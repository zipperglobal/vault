{
  "name": "zippie-vault",
  "version": "1.0.0",
  "description": "ServiceWorker based key store",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-dev": "webpack; openssl req -x509 -newkey rsa:4096 -nodes -keyout key.pem -out cert.pem -days 365 -subj \"/C=US/ST=./L=./O=./CN=.\"",
    "dev": "webpack; cd dist; http-server -p 8443 -S -K ../key.pem -C ../cert.pem"
  },
  "author": "Zippie Ltd.",
  "license": "Commercial/AGPLv3",
  "dependencies": {
    "eccrypto": "^1.0.3",
    "hdkey": "^0.8.0",
    "js-cookie": "^2.2.0",
    "secp256k1": "^3.5.0",
    "secrets.js-grempe": "^1.1.0",
    "sha.js": "^2.4.10",
    "store": "^2.0.12",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "xhr-promise": "^2.0.0"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "babel-loader": "^8.0.0-beta.3",
    "babel-polyfill": "^6.26.0"
  }
}
